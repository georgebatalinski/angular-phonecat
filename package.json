    "@angular/core": "^11.0.9",
    "@angular/forms": "^11.0.9",
    "@angular/platform-browser": "^11.0.9",
    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/router": "^11.0.9",
    "@angular/upgrade": "^11.0.9",
    "@uirouter/angularjs": "^1.0.29",
    "angular": "1.8.x",
    "angular-animate": "1.8.x",
    "angular-resource": "^1.8.2",
    "bootstrap": "3.3.x",
    "jquery": "3.3.x",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/angular": "^1.8.0",
    "@types/angular-animate": "^1.5.10",
    "@types/angular-cookies": "^1.8.0",
    "@types/angular-mocks": "^1.7.0",
    "@types/angular-resource": "^1.5.16",
    "@types/angular-route": "^1.7.1",
    "@types/angular-sanitize": "^1.8.0",
    "@types/jasmine": "^3.6.2",
    "angular-mocks": "1.8.x",
    "cpx": "^1.5.0",
    "html-loader": "^1.3.2",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.5.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^1.1.2",
    "protractor": "^5.4.4",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "zone.js": "^0.11.3"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist,jquery/dist}/**/*\" app/lib -C",
    "start": "npm run build & npm run test",
    "build": "npx webpack",
    "test": "http-server ./dist -a localhost -p 8000 -c-1",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
